<% if user_signed_in? %>
  <%= form_with url: search_path, mrthod: :get, local: true do |f| %>
    <%= f.text_field :contenr %>
    <%= f.select :model, options_for_select({"User" => "user", "Book" => "book"}) %>
    <%= f.select :method, options_for_select({"完全一致" => "perfict", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partial" }) %>
    <%= f.submit "検索" %>
  <% end %>
<% end %>